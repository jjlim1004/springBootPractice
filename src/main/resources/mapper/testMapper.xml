<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bootWeb.mapper.TestMapper">
    <select id="get" parameterType="String" resultType="com.example.bootWeb.domain.TestVo">
        select * from testVo where id = #{id}
    </select>

    <insert id="insert" parameterType="com.example.bootWeb.domain.TestVo">
        <selectKey keyProperty="no" resultType="int" order="BEFORE">
            SELECT testVo_seq.NEXTVAL FROM DUAL
        </selectKey>
        insert into testVo values(
            #{no},
            #{id},
            #{pw}
        )
    </insert>
</mapper>

